{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Diagnostics": "Critical" 
    }
  },
  "LokiHttpClient": {
    "BaseAddress": "http://localhost:3100"
  },
  "Loki": {
    "AppName": "console-app",
    "BatchPostingLimit": 1,
    "Labels": {
      "app": "console-app"
    },
    "PropertiesAsLabels": [
      "level"
    ]
  }
}
